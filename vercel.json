{
  "version": 2,
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.ts"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.ts"
    },
    {
      "src": "/api/scheduled-schedule",
      "dest": "/api/scheduled-schedule.ts"
    },
    {
      "src": "/api/scheduled-whosplaying",
      "dest": "/api/scheduled-whosplaying.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/scheduled-schedule",
      "schedule": "11 10 * * 3"
    },
    {
      "path": "/api/scheduled-whosplaying",
      "schedule": "33 14 * * 2"
    },
    {
      "path": "/api/scheduled-whosplaying",
      "schedule": "33 8 * * 6"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 